 if ch == '-':
  print(n1 ,'-', n2 ,'=',sub(a,b))
  
 if ch == '*':
  print(n1 ,'*', n2 ,'=',mul(a,b))
  
 if ch == '/':
  print(n1 ,'/', n2 ,'=',div(a,b))
  
 else:
 print("invalid choice")
  
  
