 5
 a=int(input())
 b=int(input())
 oper=input()
 match oper:
     case "+":
         print(a+b)
         break
     case "-":
         print(a-b)
         break
     case "*":
         print(a*b)
         break
     case "/":
         print(a/b)
         break
        

