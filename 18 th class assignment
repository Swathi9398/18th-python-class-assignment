a=int(input())
b=int(input())
c=input("enter operator: ")
if c=="+":
  print("addition: ",a+b)
elif  c=="-":
 print("subtraction: ",a-b)
elif  c=="*":
 print("multiplication: ",a*b)
elif c=="/":
 print("division: ",a/b)
elif c=="%":
 print("modulo: ",a%b)
else:
 print("enter valid")


